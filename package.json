{
  "name": "ohdear-npm-audit",
  "version": "0.1.1",
  "description": "Oh Dear Application Health check for npm audit critical vulnerabilities",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/handler.d.ts",
      "import": "./dist/handler.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js"
    }
  },
  "bin": {
    "ohdear-deps-manifest": "./dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "ohdear",
    "health-check",
    "npm",
    "vulnerabilities",
    "security",
    "nextjs"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "next": ">=14"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  }
}
